<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="pnRoot" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lu.uni.lassy.excalibur.examples.icrash.dev.view.gui.coordinator.QualitySurveyGUIController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="600.0">
         <bottom>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="50.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Button id="bttnQSCancel" fx:id="bttnQSCancel" mnemonicParsing="false" onAction="#bttnQSCancel_OnClick" prefHeight="50.0" prefWidth="100.0" text="Cancel">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button id="bttnQSSubmit" fx:id="bttnQSSubmit" mnemonicParsing="false" onAction="#bttnQSSubmit_OnClick" prefHeight="50.0" prefWidth="100.0" text="Submit">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </bottom>
         <top>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Survey on the quality of the iCrash system" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Text>
         </top>
         <center>
            <ScrollPane BorderPane.alignment="CENTER">
              <content>
                <AnchorPane fx:id="pnQuestionList" minHeight="0.0" minWidth="0.0">
                     <children>
                        <VBox fx:id="vbQuestionList" prefHeight="525.0" prefWidth="593.0" />
                     </children></AnchorPane>
              </content>
            </ScrollPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
